<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net8.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <Title>KetKat.JWTAuth</Title>
    <Description>This is a .Net library to generate and validate JWT Auth access and refresh token.</Description>
    <RepositoryUrl>https://github.com/ketankatdare/KetKat.JWTAuth</RepositoryUrl>
    <Version>1.0.0</Version>
    <Authors>Ketan Katdare</Authors>
    <Company>Ketan Katdare</Company>
    <PackageTags>jwt, security, token</PackageTags>
  </PropertyGroup>

</Project>
